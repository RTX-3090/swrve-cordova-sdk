<?xml version='1.0' encoding='utf-8'?>
<widget id="com.swrve.cordova.tests" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>UnitTests</name>
    <description>
        Swrve Cordova SDK Test Project
    </description>
    <author email="support@swrve.com" href="http://cordova.io"> Swrve Inc </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="local" />
    </platform>
    <plugin name="cordova-plugin-swrve" spec="../SwrvePlugin" />
    <plugin name="cordova-plugin-swrvegeo" spec="../SwrveGeoPlugin" />
</widget>
